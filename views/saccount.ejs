<!doctype html>
<html lang="en">

<head>
  <title>social</title>
  <link rel="icon" href="/static/images/hacker.png">
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS -->

  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  <link rel="stylesheet" href="/static/css/saccount.css">


  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/boxicons@latest/css/boxicons.min.css">
  <title>Hello, world!</title>
</head>

<body id="body-pd">
  <header class="header" id="header">
    <div class="header_toggle"> <i class='bx bx-menu' id="header-toggle"></i> </div>
    <div class="header_img"> <img src="/static/images/hacker.png" alt=""> </div>
  </header>
  <div class="l-navbar" id="nav-bar">
    <nav class="nav">
      <div> <a href="" class="nav_logo"> <i class='bx bxl-nodejs nav_logo-icon'></i> <span
            class="nav_logo-name">HEREiAM</span> </a>
        <div class="nav_list"> <a href=" /" class="nav_link active"> <i class='bx bx-home nav_icon'></i> <span
              class="nav_name">HOME</span> </a>
          <a href="gallery" class="nav_link"> <i class='bx bx-collection nav_icon'></i> <span class="nav_name"
              id="gallery">Gallery</span>
          </a> <a href="certificates" class="nav_link"> <i class='bx bx-message-square-detail nav_icon'></i> <span
              class="nav_name">Certificates</span> </a>
          <a href="saccount" class="nav_link"> <i class='bx bx-copy-alt nav_icon'></i> <span
              class="nav_name">SocialAccounts</span> </a>
          <a href="chat" class="nav_link"> <i class='bx bxs-chat nav_icon'></i> <span class="nav_name">G-chat</span>
          </a> <a href="#" class="nav_link"> <i class='bx bxs-contact nav_icon'></i> <span
              class="nav_name">contacts</span> </a>
        </div>
      </div> <a href="#" class="nav_link"> <i class='bx bx-log-out nav_icon'></i> <span class="nav_name">Back</span>
      </a>
    </nav>
  </div>
  <!--Container Main start-->
  <div class="height-100 bg-light">
    <div class="container text-center">
      <div class="row">
        <% socialdata.forEach((socialdata)=> { %>
          <div class="col">
            <div id="cardID" class="card" style="width: 18rem;">
              <img src=<%=socialdata.image %> class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title">
                  <%= socialdata.name %>
                </h5>
                <p class="card-text">
                  <%= socialdata.idname %>
                </p>
              </div>
              <ul class="list-group list-group-flush">
                <li class="list-group-item">
                  <%= socialdata.name %>
                </li>
                <li class="list-group-item">
                  <%= socialdata.idname %>
                </li>
                <li class="list-group-item">check it out</li>
              </ul>
              <div class="card-body">
                <a href=<%=socialdata.url %> target="_blank" class="card-link">wana go</a>
                <a href=<%=socialdata.url %> target="_blank" class="card-link">here i am too</a>
              </div>
            </div>
          </div>
          <% }) %>
      </div>
    </div>
    <%- include('common/footer')%>
  </div>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
    crossorigin="anonymous"></script>
  <script src="/static/js/account.js"></script>
</body>

</html>